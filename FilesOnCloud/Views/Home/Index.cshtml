@{
    ViewBag.Title = "Upload Page";
}

<div class="jumbotron">
    @using (Html.BeginForm("Index", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()
        <input type="file" name="files" />
        <button type="submit" value="Upload">Upload</button>
    }
</div>
